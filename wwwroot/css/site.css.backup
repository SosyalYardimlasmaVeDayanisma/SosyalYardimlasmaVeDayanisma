/* ============================================
   İYİLİK PUAN - ÖZGÜN VE ZARİF TASARIM
   İnsanı, Doğayı, Yardımlaşmayı Temsil Eden
   Canlı, Dinamik ve Profesyonel Arayüz
   ============================================ */

* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

:root {
    /* ============================================
       RENK PALETİ - Doğa ve Yardımlaşma Temalı
       Ana Renk: Doğa Yeşili (#10B981)
       Vurgu Rengi: Sıcak Turuncu (sadece CTA'lar için)
       ============================================ */
    
    /* Ana Renk - Doğa Yeşili (Büyüme, Umut, Yenilenme) */
    --primary: #10B981;
    --primary-light: #34D399;
    --primary-lighter: #6EE7B7;
    --primary-dark: #059669;
    --primary-darker: #047857;
    --primary-subtle: rgba(16, 185, 129, 0.1);
    --primary-glow: rgba(16, 185, 129, 0.25);
    
    /* Vurgu Rengi - Sıcak Amber (Sadece önemli butonlar ve CTA'lar için) */
    --accent: #F59E0B;
    --accent-light: #FBBF24;
    --accent-dark: #D97706;
    
    /* Başarı, Hata ve Uyarı Renkleri */
    --success: #10B981;
    --success-light: #D1FAE5;
    --error: #EF4444;
    --error-light: #FEE2E2;
    --warning: #F59E0B;
    --warning-light: #FEF3C7;
    --info: #3B82F6;
    --info-light: #DBEAFE;
    
    /* Gradient'ler - Minimal ve Tutarlı */
    --gradient-primary: linear-gradient(135deg, #10B981 0%, #059669 100%);
    --gradient-subtle: linear-gradient(135deg, rgba(16, 185, 129, 0.05) 0%, rgba(16, 185, 129, 0.02) 100%);
    --gradient-hero: linear-gradient(180deg, #FFFFFF 0%, #F8FAFC 100%);
    
    /* Metin Renkleri - Okunabilir ve Tutarlı */
    --text-primary: #0F172A;
    --text-secondary: #475569;
    --text-tertiary: #64748B;
    --text-muted: #94A3B8;
    --text-inverse: #FFFFFF;
    
    /* Arka Planlar - Temiz ve Modern */
    --bg-primary: #FFFFFF;
    --bg-secondary: #F8FAFC;
    --bg-tertiary: #F1F5F9;
    --bg-success: #F0FDF4;
    --bg-warning: #FFFBEB;
    --bg-error: #FEF2F2;
    
    /* Kenarlıklar - Yumuşak ve Tutarlı */
    --border: #E2E8F0;
    --border-light: #F1F5F9;
    --border-primary: rgba(16, 185, 129, 0.2);
    
    /* Gölgeler - Modern ve Derinlik Hissi */
    --shadow-xs: 0 1px 2px 0 rgba(0, 0, 0, 0.05);
    --shadow-sm: 0 2px 8px -1px rgba(0, 0, 0, 0.08), 0 1px 4px -1px rgba(0, 0, 0, 0.04);
    --shadow-md: 0 8px 16px -4px rgba(0, 0, 0, 0.1), 0 4px 8px -2px rgba(0, 0, 0, 0.06);
    --shadow-lg: 0 16px 32px -8px rgba(0, 0, 0, 0.12), 0 8px 16px -4px rgba(0, 0, 0, 0.08);
    --shadow-xl: 0 24px 48px -12px rgba(0, 0, 0, 0.15), 0 12px 24px -6px rgba(0, 0, 0, 0.1);
    --shadow-primary: 0 8px 20px -4px rgba(16, 185, 129, 0.3);
    
    /* Geçişler - Pürüzsüz ve Tutarlı */
    --transition-fast: 150ms cubic-bezier(0.4, 0, 0.2, 1);
    --transition-smooth: 250ms cubic-bezier(0.4, 0, 0.2, 1);
    --transition-elegant: 350ms cubic-bezier(0.25, 0.46, 0.45, 0.94);
    
    /* Border Radius - Modern ve Tutarlı */
    --radius-sm: 8px;
    --radius-md: 12px;
    --radius-lg: 16px;
    --radius-xl: 20px;
    --radius-2xl: 24px;
    --radius-full: 9999px;
}

/* ============================================
   GLOBAL ANIMATIONS - Sayfa Yükleme
   ============================================ */
@keyframes pageLoad {
    from {
        opacity: 0;
        transform: translateY(10px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

body {
    font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;
    background: var(--gradient-hero);
    background-attachment: fixed;
    color: var(--text-primary);
    line-height: 1.6;
    font-size: 14px;
    font-weight: 400;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    overflow-x: hidden;
    animation: pageLoad 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94);
}

/* Modern scroll behavior */
html {
    scroll-behavior: smooth;
}

html::-webkit-scrollbar {
    width: 12px;
}

html::-webkit-scrollbar-track {
    background: var(--bg-secondary);
}

html::-webkit-scrollbar-thumb {
    background: var(--gradient-primary);
    border-radius: var(--radius-full);
    border: 3px solid var(--bg-secondary);
}

html::-webkit-scrollbar-thumb:hover {
    background: var(--primary-dark);
}

/* ============================================
   CONTAINER & LAYOUT
   ============================================ */

.container {
    max-width: 1320px;
    margin: 0 auto;
    padding: 0 24px;
}

.page-container {
    padding: 64px 24px;
}

@media (min-width: 768px) {
    .page-container {
        padding: 96px 24px;
    }
}

/* ============================================
   HEADER - Modern ve Dinamik
   ============================================ */

header {
    position: sticky;
    top: 0;
    z-index: 1000;
    width: 100%;
    background: rgba(255, 255, 255, 0.85);
    backdrop-filter: blur(24px) saturate(200%);
    -webkit-backdrop-filter: blur(24px) saturate(200%);
    border-bottom: 1px solid var(--border-light);
    box-shadow: var(--shadow-sm);
    transition: all var(--transition-smooth);
}

header.scrolled {
    background: rgba(255, 255, 255, 0.95);
    box-shadow: var(--shadow-lg);
    border-bottom-color: var(--border);
}

.header-content {
    display: flex;
    height: 88px;
    align-items: center;
    justify-content: space-between;
    gap: 32px;
}

.logo {
    display: flex;
    align-items: center;
    gap: 16px;
    text-decoration: none;
    color: var(--text-primary);
    transition: opacity var(--transition-smooth);
}

.logo:hover {
    opacity: 0.85;
}

.logo-icon {
    width: 56px;
    height: 56px;
    border-radius: var(--radius-md);
    background: var(--gradient-primary);
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: var(--shadow-sm);
    transition: all var(--transition-smooth);
}

.logo:hover .logo-icon {
    box-shadow: var(--shadow-md);
    transform: translateY(-2px);
}

.logo-icon svg {
    width: 32px;
    height: 32px;
    color: white;
    filter: drop-shadow(0 1px 2px rgba(0, 0, 0, 0.1));
}

.logo-text {
    font-size: 24px;
    font-weight: 800;
    background: var(--gradient-primary);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    letter-spacing: -0.5px;
}

nav {
    display: flex;
    align-items: center;
    gap: 4px;
}

@media (max-width: 991px) {
    nav {
        display: none;
    }
}

nav a {
    font-size: 15px;
    font-weight: 600;
    color: var(--text-secondary);
    text-decoration: none;
    padding: 14px 22px;
    border-radius: var(--radius-md);
    transition: all var(--transition-smooth);
    position: relative;
    overflow: hidden;
}

nav a::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: var(--gradient-soft);
    opacity: 0;
    transition: opacity var(--transition-smooth);
    z-index: -1;
}

nav a::after {
    content: '';
    position: absolute;
    bottom: 8px;
    left: 22px;
    right: 22px;
    height: 3px;
    background: var(--gradient-primary);
    border-radius: var(--radius-full);
    transform: scaleX(0);
    transform-origin: left;
    transition: transform var(--transition-elegant);
}

nav a:hover {
    color: var(--primary);
    transform: translateY(-2px);
}

nav a:hover::before {
    opacity: 1;
}

nav a.active {
    color: var(--primary);
    background: var(--gradient-soft);
}

nav a.active::after {
    transform: scaleX(1);
}

.header-actions {
    display: flex;
    align-items: center;
    gap: 12px;
}

.notification-btn {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    background: rgba(16, 185, 129, 0.1);
    color: var(--primary);
    transition: all 0.3s ease;
    text-decoration: none;
}

.notification-btn:hover {
    background: var(--primary);
    color: white;
    transform: scale(1.1);
}

.notification-badge {
    position: absolute;
    top: -4px;
    right: -4px;
    background: var(--accent);
    color: white;
    font-size: 0.7rem;
    font-weight: 700;
    padding: 2px 6px;
    border-radius: 10px;
    min-width: 18px;
    text-align: center;
}

@media (max-width: 991px) {
    .header-actions {
        display: none;
    }
}

/* Mobile Menu Button */
.mobile-menu-btn {
    display: none;
    flex-direction: column;
    gap: 6px;
    background: none;
    border: none;
    cursor: pointer;
    padding: 8px;
    z-index: 1001;
}

@media (max-width: 991px) {
    .mobile-menu-btn {
        display: flex;
    }
}

.mobile-menu-btn span {
    display: block;
    width: 28px;
    height: 3px;
    background: var(--primary);
    border-radius: 2px;
    transition: all 0.3s ease;
}

.mobile-menu-btn.active span:nth-child(1) {
    transform: rotate(45deg) translate(8px, 8px);
}

.mobile-menu-btn.active span:nth-child(2) {
    opacity: 0;
}

.mobile-menu-btn.active span:nth-child(3) {
    transform: rotate(-45deg) translate(7px, -7px);
}

/* Mobile Menu */
.mobile-menu {
    display: none;
    position: fixed;
    top: 88px;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(255, 255, 255, 0.98);
    backdrop-filter: blur(20px);
    z-index: 999;
    overflow-y: auto;
    transform: translateX(-100%);
    transition: transform 0.3s ease;
}

.mobile-menu.active {
    transform: translateX(0);
}

.mobile-menu nav {
    display: flex;
    flex-direction: column;
    padding: 24px;
    gap: 8px;
}

.mobile-menu nav a {
    padding: 16px 20px;
    font-size: 16px;
    border-radius: var(--radius-md);
    border: 1px solid var(--border-light);
    text-align: center;
}

.mobile-menu .header-actions {
    display: flex;
    flex-direction: column;
    padding: 24px;
    gap: 12px;
    border-top: 1px solid var(--border-light);
    margin-top: 16px;
}

.mobile-menu .header-actions .btn {
    width: 100%;
    justify-content: center;
}


/* ============================================
   BUTTONS - Modern ve Temiz
   ============================================ */

.btn {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    padding: 12px 24px;
    border-radius: var(--radius-md);
    font-size: 14px;
    font-weight: 600;
    text-decoration: none;
    border: none;
    cursor: pointer;
    transition: all var(--transition-smooth);
    white-space: nowrap;
    box-shadow: var(--shadow-sm);
    letter-spacing: 0.02em;
}

.btn:hover {
    transform: translateY(-2px);
    box-shadow: var(--shadow-md);
}

.btn:active {
    transform: translateY(0);
    box-shadow: var(--shadow-sm);
}

.btn-primary {
    background: var(--gradient-primary);
    color: var(--text-inverse);
}

.btn-primary:hover {
    box-shadow: var(--shadow-primary);
}

.btn-outline {
    border: 2px solid var(--border-primary);
    background: var(--bg-primary);
    color: var(--primary);
}

.btn-outline:hover {
    background: var(--primary-subtle);
    border-color: var(--primary);
    color: var(--primary-dark);
}

.btn-sm {
    padding: 8px 16px;
    font-size: 13px;
    height: 36px;
}

.btn-lg {
    padding: 14px 28px;
    font-size: 16px;
    height: 52px;
    border-radius: var(--radius-lg);
    font-weight: 700;
}

.btn-full {
    width: 100%;
}

/* ============================================
   CARDS - Modern ve Temiz
   ============================================ */

.card {
    border-radius: var(--radius-lg);
    border: 1px solid var(--border-light);
    background: var(--bg-primary);
    box-shadow: var(--shadow-sm);
    transition: all var(--transition-smooth);
    overflow: hidden;
}

.card:hover {
    box-shadow: var(--shadow-lg);
    transform: translateY(-4px);
    border-color: var(--border-primary);
}

.card-header {
    padding: 20px 24px;
    border-bottom: 1px solid var(--border-light);
    background: var(--bg-secondary);
}

.card-body {
    padding: 24px;
}

.card-footer {
    padding: 20px 24px;
    border-top: 1px solid var(--border-light);
    background: var(--bg-secondary);
}

/* ============================================
   TYPOGRAPHY - Temiz ve Okunabilir
   ============================================ */

.page-title {
    font-size: 32px;
    font-weight: 800;
    margin-bottom: 16px;
    line-height: 1.2;
    color: var(--text-primary);
    letter-spacing: -0.02em;
}

@media (min-width: 768px) {
    .page-title {
        font-size: 40px;
    }
}

@media (min-width: 1024px) {
    .page-title {
        font-size: 56px;
    }
}

.page-description {
    color: var(--text-secondary);
    font-size: 16px;
    margin-bottom: 40px;
    line-height: 1.8;
    max-width: 720px;
    font-weight: 400;
}

.verification-badge {
    display: inline-flex;
    align-items: center;
    padding: 8px 16px;
    background: linear-gradient(135deg, rgba(16, 185, 129, 0.15), rgba(16, 185, 129, 0.08));
    color: var(--primary-dark);
    border-radius: 20px;
    font-size: 0.9rem;
    font-weight: 600;
    border: 2px solid rgba(16, 185, 129, 0.3);
    white-space: nowrap;
}

.verification-info {
    margin-bottom: 1.5rem;
    padding: 1rem;
    background: rgba(16, 185, 129, 0.05);
    border-left: 3px solid var(--primary);
    border-radius: 8px;
    font-size: 0.95rem;
    color: var(--text-secondary);
}

.verification-info strong {
    color: var(--primary-dark);
}

@media (min-width: 768px) {
    .page-description {
        font-size: 18px;
    }
}

h1, h2, h3, h4, h5, h6 {
    font-weight: 800;
    line-height: 1.3;
    color: var(--text-primary);
    letter-spacing: -0.03em;
}

h1 {
    font-size: 32px;
    font-weight: 900;
}

h2 {
    font-size: 28px;
    margin-bottom: 28px;
    font-weight: 800;
}

h3 {
    font-size: 22px;
    margin-bottom: 20px;
    font-weight: 700;
}

/* ============================================
   HERO SECTION
   ============================================ */

.hero {
    text-align: center;
    margin-bottom: 80px;
    padding: 80px 32px;
    background: linear-gradient(135deg, rgba(16, 185, 129, 0.08), rgba(16, 185, 129, 0.02));
    border-radius: var(--radius-2xl);
    border: 1px solid var(--border-light);
}

.hero h1 {
    font-size: 42px;
    font-weight: 900;
    margin-bottom: 24px;
    line-height: 1.1;
    color: var(--primary-color);
}

@media (min-width: 768px) {
    .hero h1 {
        font-size: 56px;
    }
}

@media (min-width: 1024px) {
    .hero h1 {
        font-size: 72px;
    }
}

.hero p {
    font-size: 18px;
    color: var(--text-secondary);
    margin-bottom: 56px;
    max-width: 860px;
    margin-left: auto;
    margin-right: auto;
    line-height: 1.8;
    animation: heroTextSlide 1.2s cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.2s both;
    font-weight: 400;
}

@keyframes heroTextSlide {
    from {
        opacity: 0;
        transform: translateY(40px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

@media (min-width: 768px) {
    .hero p {
        font-size: 24px;
    }
}

.hero-actions {
    display: flex;
    gap: 24px;
    justify-content: center;
    flex-wrap: wrap;
    animation: heroActionsSlide 1.4s cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.4s both;
}

@keyframes heroActionsSlide {
    from {
        opacity: 0;
        transform: translateY(30px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

/* ============================================
   STATS GRID - Dinamik ve Etkileyici
   ============================================ */

.stats-grid {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 32px;
    margin-bottom: 96px;
}

@media (min-width: 768px) {
    .stats-grid {
        grid-template-columns: repeat(4, 1fr);
        gap: 40px;
    }
}

.stat-card {
    text-align: center;
    padding: 28px 24px;
    background: var(--bg-primary);
    border-radius: var(--radius-lg);
    border: 1px solid var(--border-light);
    box-shadow: var(--shadow-sm);
    transition: all var(--transition-smooth);
    position: relative;
    overflow: hidden;
    min-height: 160px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.stat-card:hover {
    transform: translateY(-4px);
    box-shadow: var(--shadow-lg);
    border-color: var(--primary-light);
}

.stat-icon {
    width: 48px;
    height: 48px;
    margin: 0 auto 16px;
    color: var(--primary);
    background: rgba(16, 185, 129, 0.1);
    border-radius: var(--radius-lg);
    display: flex;
    align-items: center;
    justify-content: center;
    transition: all var(--transition-smooth);
    flex-shrink: 0;
}

.stat-card:hover .stat-icon {
    background: var(--primary);
    color: white;
}

.stat-value {
    font-size: 24px;
    font-weight: 700;
    margin-bottom: 8px;
    color: var(--primary);
    line-height: 1.2;
}

.stat-label {
    font-size: 12px;
    color: var(--text-secondary);
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    line-height: 1.4;
}

/* ============================================
   CAMPAIGN CARDS - ZARİF VE ÇEKİCİ
   ============================================ */

/* Kampanya kartları için grid sistemi */
.campaigns-list {
    display: grid;
    grid-template-columns: 1fr;
    gap: 28px;
    margin-bottom: 60px;
}

@media (min-width: 768px) {
    .campaigns-list {
        grid-template-columns: repeat(2, 1fr);
        gap: 32px;
    }
}

.campaign-card {
    margin-bottom: 0;
    position: relative;
}

.campaign-card-header {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    margin-bottom: 20px;
}

.campaign-title {
    font-size: 22px;
    font-weight: 800;
    margin-bottom: 16px;
    color: var(--text-primary);
    line-height: 1.3;
    letter-spacing: -0.03em;
}

.campaign-meta {
    display: flex;
    align-items: center;
    gap: 14px;
    font-size: 14px;
    color: var(--text-secondary);
    margin-bottom: 20px;
    flex-wrap: wrap;
}

.campaign-meta svg {
    width: 18px;
    height: 18px;
    color: var(--primary);
}

.campaign-progress {
    margin: 24px 0;
}

.progress-bar {
    width: 100%;
    height: 20px;
    background-color: var(--bg-tertiary);
    border-radius: var(--radius-full);
    overflow: hidden;
    position: relative;
    box-shadow: inset 0 2px 6px rgba(0, 0, 0, 0.08);
    border: 1px solid var(--border-light);
}

.progress-fill {
    height: 100%;
    background: var(--gradient-primary);
    border-radius: var(--radius-full);
    transition: width 1.8s cubic-bezier(0.65, 0, 0.35, 1);
    position: relative;
    overflow: hidden;
    box-shadow: 0 2px 8px rgba(16, 185, 129, 0.4), inset 0 1px 2px rgba(255, 255, 255, 0.3);
}

.progress-fill::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.5), transparent);
    animation: progressShimmer 2s infinite;
}

.progress-fill::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 50%;
    background: linear-gradient(180deg, rgba(255, 255, 255, 0.2) 0%, transparent 100%);
}

@keyframes progressShimmer {
    0% { transform: translateX(-100%); }
    100% { transform: translateX(100%); }
}

.campaign-progress-info {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 14px;
    margin-top: 16px;
}

.campaign-progress-amount {
    font-weight: 800;
    color: var(--text-primary);
    font-size: 18px;
}

.campaign-ngo {
    display: flex;
    align-items: center;
    gap: 12px;
    font-size: 14px;
    color: var(--text-secondary);
    margin-top: 20px;
    padding: 16px;
    background: var(--gradient-soft);
    border-radius: var(--radius-md);
    border: 1px solid var(--border-primary);
}

.campaign-ngo svg {
    width: 18px;
    height: 18px;
    color: var(--primary);
}

/* ============================================
   BADGES - Premium ve Belirgin
   ============================================ */

.badge {
    display: inline-flex;
    align-items: center;
    padding: 10px 20px;
    border-radius: var(--radius-full);
    font-size: 13px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 0.8px;
    box-shadow: var(--shadow-sm);
    transition: all var(--transition-smooth);
}

.badge-active {
    background: var(--primary);
    color: white;
    box-shadow: 0 2px 8px -2px rgba(16, 185, 129, 0.4);
}

.badge-completed {
    background: var(--bg-tertiary);
    color: var(--text-secondary);
}

.badge-pending {
    background: var(--accent);
    color: white;
    box-shadow: 0 2px 8px -2px rgba(245, 158, 11, 0.4);
}

/* ============================================
   FORMS - ZARİF VE KULLANICI DOSTU
   ============================================ */

.form-group {
    margin-bottom: 32px;
}

.form-label {
    display: block;
    font-size: 16px;
    font-weight: 700;
    margin-bottom: 14px;
    color: var(--text-primary);
}

.form-input,
.form-select {
    width: 100%;
    height: 60px;
    padding: 0 24px;
    border-radius: var(--radius-md);
    border: 2px solid var(--border);
    background-color: var(--bg-primary);
    font-size: 16px;
    transition: all var(--transition-smooth);
    font-family: inherit;
    box-shadow: var(--shadow-sm);
}

.form-input:focus,
.form-select:focus {
    outline: none;
    border-color: var(--primary);
    box-shadow: var(--shadow-lg), 0 0 0 4px rgba(16, 185, 129, 0.12);
    background-color: var(--bg-primary);
    transform: translateY(-2px);
}

.form-input::placeholder {
    color: var(--text-tertiary);
}

/* ============================================
   SEARCH & FILTERS
   ============================================ */

.filters-container {
    padding: 52px;
    background: var(--bg-primary);
    border-radius: var(--radius-xl);
    margin-bottom: 64px;
    box-shadow: var(--shadow-lg);
    border: 1px solid var(--border-light);
}

.search-input-wrapper {
    position: relative;
    margin-bottom: 32px;
}

.search-input-wrapper svg {
    position: absolute;
    left: 24px;
    top: 50%;
    transform: translateY(-50%);
    width: 24px;
    height: 24px;
    color: var(--text-tertiary);
    pointer-events: none;
}

.search-input {
    width: 100%;
    height: 60px;
    padding-left: 64px;
    padding-right: 24px;
    border-radius: var(--radius-md);
    border: 2px solid var(--border);
    background-color: var(--bg-secondary);
    font-size: 16px;
    transition: all var(--transition-smooth);
    box-shadow: var(--shadow-sm);
}

.search-input:focus {
    background-color: var(--bg-primary);
    border-color: var(--primary);
    box-shadow: 0 4px 12px -2px rgba(0, 0, 0, 0.08), 0 0 0 4px rgba(16, 185, 129, 0.1);
}

.filters-grid {
    display: grid;
    grid-template-columns: 1fr;
    gap: 28px;
    margin-bottom: 32px;
}

@media (min-width: 768px) {
    .filters-grid {
        grid-template-columns: repeat(3, 1fr);
    }
}

.filter-actions {
    display: flex;
    gap: 20px;
}

/* ============================================
   GRID LAYOUTS
   ============================================ */

.grid {
    display: grid;
    gap: 40px;
}

.grid-2 {
    grid-template-columns: 1fr;
}

.grid-3 {
    grid-template-columns: 1fr;
}

.grid-4 {
    grid-template-columns: repeat(2, 1fr);
}

@media (min-width: 768px) {
    .grid-2 {
        grid-template-columns: repeat(2, 1fr);
    }
    
    .grid-3 {
        grid-template-columns: repeat(2, 1fr);
    }
    
    .grid-4 {
        grid-template-columns: repeat(4, 1fr);
    }
}

@media (min-width: 1024px) {
    .grid-3 {
        grid-template-columns: repeat(3, 1fr);
    }
}

.content-grid {
    display: grid;
    grid-template-columns: 1fr;
    gap: 56px;
}

@media (min-width: 1024px) {
    .content-grid {
        grid-template-columns: 2fr 1fr;
    }
}

/* ============================================
   WALLET WIDGET - CANLI GRADIENT
   ============================================ */

.wallet-widget {
    margin-bottom: 40px;
    background: var(--gradient-primary);
    border: none;
    color: var(--text-inverse);
    position: relative;
    overflow: hidden;
    box-shadow: var(--shadow-2xl), var(--shadow-primary-lg);
    transition: all var(--transition-elegant);
}

.wallet-widget:hover {
    transform: translateY(-4px) scale(1.01);
    box-shadow: 0 32px 64px -16px rgba(16, 185, 129, 0.4), 0 0 64px -24px rgba(16, 185, 129, 0.3);
}

.wallet-widget::before {
    content: '';
    position: absolute;
    top: -60%;
    right: -30%;
    width: 600px;
    height: 600px;
    background: rgba(255, 255, 255, 0.15);
    border-radius: 50%;
    animation: walletFloat 12s ease-in-out infinite;
}

@keyframes walletFloat {
    0%, 100% { 
        transform: translate(0, 0) scale(1); 
        opacity: 0.5;
    }
    50% { 
        transform: translate(-50px, 50px) scale(1.2); 
        opacity: 0.8;
    }
}

.wallet-widget .card-body {
    position: relative;
    z-index: 1;
}

.wallet-header {
    display: flex;
    align-items: center;
    gap: 18px;
    margin-bottom: 32px;
}

.wallet-header svg {
    width: 36px;
    height: 36px;
    color: rgba(255, 255, 255, 0.95);
    filter: drop-shadow(0 2px 4px rgba(0, 0, 0, 0.15));
}

.wallet-header h3 {
    font-size: 24px;
    font-weight: 800;
    color: rgba(255, 255, 255, 0.95);
}

.wallet-tier {
    font-size: 14px;
    margin-bottom: 14px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    color: rgba(255, 255, 255, 0.9);
}

.wallet-balance {
    font-size: 64px;
    font-weight: 900;
    color: var(--text-inverse);
    margin-bottom: 10px;
    text-shadow: 0 2px 8px rgba(0, 0, 0, 0.2);
}

.wallet-stats {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 28px;
    padding-top: 36px;
    border-top: 1px solid rgba(255, 255, 255, 0.3);
    margin-bottom: 32px;
}

.wallet-stat {
    display: flex;
    flex-direction: column;
}

.wallet-stat-label {
    display: flex;
    align-items: center;
    gap: 10px;
    font-size: 15px;
    font-weight: 600;
    margin-bottom: 12px;
    color: rgba(255, 255, 255, 0.9);
}

.wallet-stat-label svg {
    width: 20px;
    height: 20px;
}

.wallet-stat-value {
    font-size: 28px;
    font-weight: 800;
    color: var(--text-inverse);
}

.wallet-transactions {
    padding-top: 32px;
    border-top: 1px solid rgba(255, 255, 255, 0.3);
}

.wallet-transaction {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 18px;
    background: rgba(255, 255, 255, 0.15);
    backdrop-filter: blur(10px);
    border-radius: var(--radius-md);
    margin-bottom: 14px;
    transition: all var(--transition-smooth);
    border: 1px solid rgba(255, 255, 255, 0.2);
}

.wallet-transaction:hover {
    background: rgba(255, 255, 255, 0.25);
    transform: translateX(8px);
}

.wallet-transaction-info {
    flex: 1;
}

.wallet-transaction-title {
    font-weight: 700;
    margin-bottom: 8px;
    color: var(--text-inverse);
    font-size: 16px;
}

.wallet-transaction-date {
    font-size: 14px;
    color: rgba(255, 255, 255, 0.85);
    display: flex;
    align-items: center;
    gap: 8px;
}

.wallet-transaction-date svg {
    width: 16px;
    height: 16px;
}

.wallet-transaction-amount {
    font-size: 24px;
    font-weight: 800;
    color: var(--text-inverse);
}

.wallet-widget .btn-outline {
    background: rgba(255, 255, 255, 0.25);
    border-color: rgba(255, 255, 255, 0.4);
    color: var(--text-inverse);
    backdrop-filter: blur(10px);
    box-shadow: var(--shadow-md);
}

.wallet-widget .btn-outline:hover {
    background: rgba(255, 255, 255, 0.35);
    border-color: rgba(255, 255, 255, 0.6);
    transform: translateY(-2px);
}

/* ============================================
   FOOTER - Sıcak ve Modern
   ============================================ */

footer {
    border-top: 1px solid var(--border);
    background: linear-gradient(180deg, var(--bg-primary) 0%, #F8FAFC 50%, #F0F4F8 100%);
    padding: 120px 24px 64px;
    margin-top: 160px;
    position: relative;
}

footer::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: var(--gradient-primary);
    box-shadow: 0 2px 16px rgba(16, 185, 129, 0.3);
}

.footer-content {
    display: grid;
    grid-template-columns: 1fr;
    gap: 64px;
    margin-bottom: 64px;
}

@media (min-width: 768px) {
    .footer-content {
        grid-template-columns: repeat(4, 1fr);
        gap: 56px;
    }
}

.footer-section h3 {
    font-weight: 800;
    margin-bottom: 32px;
    font-size: 24px;
    color: var(--text-primary);
    position: relative;
}

.footer-section h3::after {
    content: '';
    position: absolute;
    bottom: -10px;
    left: 0;
    width: 50px;
    height: 4px;
    background: var(--gradient-primary);
    border-radius: var(--radius-full);
    box-shadow: 0 2px 8px rgba(16, 185, 129, 0.3);
}

.footer-section p {
    font-size: 16px;
    color: var(--text-secondary);
    line-height: 2;
}

.footer-section ul {
    list-style: none;
}

.footer-section ul li {
    margin-bottom: 18px;
}

.footer-section ul li a {
    font-size: 16px;
    color: var(--text-secondary);
    text-decoration: none;
    transition: all var(--transition-smooth);
    display: inline-block;
    font-weight: 500;
    position: relative;
    padding-left: 20px;
}

.footer-section ul li a:hover {
    color: var(--primary);
    transform: translateX(4px);
}

.footer-bottom {
    text-align: center;
    padding-top: 56px;
    border-top: 1px solid var(--border);
    font-size: 16px;
    color: var(--text-secondary);
    font-weight: 500;
}

/* ============================================
   MOBILE MENU
   ============================================ */

.mobile-menu-btn {
    display: block;
    background: none;
    border: none;
    cursor: pointer;
    padding: 12px;
    color: var(--text-primary);
    transition: all var(--transition-smooth);
    border-radius: var(--radius-md);
}

.mobile-menu-btn:hover {
    color: var(--primary);
    background: var(--bg-secondary);
    transform: scale(1.05);
}

.mobile-menu-btn:active {
    transform: scale(0.95);
    transition: all 100ms cubic-bezier(0.4, 0, 0.2, 1);
}

.mobile-menu {
    display: none;
    border-top: 1px solid var(--border-light);
    padding: 28px 0;
    background: var(--bg-primary);
}

.mobile-menu.active {
    display: block;
}

.mobile-menu a {
    display: block;
    padding: 18px 28px;
    font-size: 16px;
    color: var(--text-secondary);
    text-decoration: none;
    transition: all var(--transition-smooth);
    border-radius: var(--radius-md);
    margin: 6px 0;
    font-weight: 600;
}

.mobile-menu a:hover {
    color: var(--primary);
    background-color: var(--gradient-soft);
    transform: translateX(4px);
}

/* ============================================
   UTILITIES
   ============================================ */

.text-center {
    text-align: center;
}

.text-primary {
    color: var(--primary);
}

.text-secondary {
    color: var(--text-secondary);
}

.mb-4 {
    margin-bottom: 16px;
}

.mb-6 {
    margin-bottom: 24px;
}

.mb-8 {
    margin-bottom: 32px;
}

.mt-4 {
    margin-top: 16px;
}

.p-4 {
    padding: 16px;
}

.p-6 {
    padding: 24px;
}

.space-y-4 > * + * {
    margin-top: 16px;
}

.space-y-6 > * + * {
    margin-top: 24px;
}

.empty-state {
    padding: 120px 40px;
    text-align: center;
    color: var(--text-secondary);
}

.empty-state p {
    font-size: 20px;
    font-weight: 500;
}

/* ============================================
   RESPONSIVE
   ============================================ */

@media (min-width: 768px) {
    nav {
        display: flex;
    }
    
    .header-actions {
        display: flex;
    }
    
    .mobile-menu-btn {
        display: none;
    }
}

/* ============================================
   ANIMATIONS - Dinamik ve Profesyonel
   ============================================ */

@keyframes fadeIn {
    from {
        opacity: 0;
        transform: translateY(30px) scale(0.98);
    }
    to {
        opacity: 1;
        transform: translateY(0) scale(1);
    }
}

/* Kart listelerinde sıralı yüklenme animasyonu */
.card {
    animation: fadeIn 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94) both;
}

.card:nth-child(1) { animation-delay: 0.05s; }
.card:nth-child(2) { animation-delay: 0.1s; }
.card:nth-child(3) { animation-delay: 0.15s; }
.card:nth-child(4) { animation-delay: 0.2s; }
.card:nth-child(5) { animation-delay: 0.25s; }
.card:nth-child(6) { animation-delay: 0.3s; }

/* Sayfa geçiş animasyonu */
@keyframes pageTransition {
    from {
        opacity: 0;
        transform: translateY(20px) scale(0.98);
    }
    to {
        opacity: 1;
        transform: translateY(0) scale(1);
    }
}

.page-container {
    animation: pageTransition 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94);
}

/* Pulse animasyonu - Dikkat çekici öğeler için */
@keyframes pulse {
    0%, 100% {
        transform: scale(1);
        opacity: 1;
    }
    50% {
        transform: scale(1.05);
        opacity: 0.9;
    }
}

/* Floating animasyonu */
@keyframes float {
    0%, 100% {
        transform: translateY(0);
    }
    50% {
        transform: translateY(-10px);
    }
}

/* ============================================
   SCROLLBAR STYLING - ZARİF
   ============================================ */

::-webkit-scrollbar {
    width: 14px;
    height: 14px;
}

::-webkit-scrollbar-track {
    background: var(--bg-secondary);
}

::-webkit-scrollbar-thumb {
    background: var(--primary);
    border-radius: var(--radius-full);
    border: 3px solid var(--bg-secondary);
}

::-webkit-scrollbar-thumb:hover {
    background: var(--primary-dark);
}

/* ============================================
   WIZARD & DONATE FORM
   ============================================ */

.wizard-container {
    max-width: 900px;
    margin: 0 auto;
}

.wizard-steps {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 48px;
    gap: 12px;
}

.wizard-step {
    width: 56px;
    height: 56px;
    border-radius: var(--radius-full);
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 20px;
    font-weight: 800;
    transition: all var(--transition-smooth);
    box-shadow: var(--shadow-md);
}

.wizard-step.active {
    background: var(--primary);
    color: white;
    box-shadow: 0 4px 12px -3px rgba(16, 185, 129, 0.4);
}

.wizard-step.inactive {
    background: var(--bg-tertiary);
    color: var(--text-tertiary);
    border: 2px solid var(--border);
}

.wizard-step-line {
    width: 120px;
    height: 4px;
    background: var(--bg-tertiary);
    border-radius: var(--radius-full);
    transition: all var(--transition-smooth);
}

.wizard-step-line.active {
    background: var(--primary);
}

.wizard-step-content {
    display: none;
    padding: 48px 0;
    animation: fadeIn 0.4s cubic-bezier(0.4, 0, 0.2, 1);
}

.wizard-step-content.active {
    display: block;
}

.wizard-actions {
    display: flex;
    align-items: center;
    gap: 20px;
    margin-top: 48px;
    padding-top: 32px;
    border-top: 2px solid var(--border-light);
}

.campaign-select-list {
    display: flex;
    flex-direction: column;
    gap: 16px;
}

.campaign-select-btn {
    width: 100%;
    padding: 24px;
    border: 2px solid var(--border);
    border-radius: var(--radius-lg);
    background: var(--bg-primary);
    text-align: left;
    cursor: pointer;
    transition: all var(--transition-smooth);
    box-shadow: var(--shadow-sm);
}

.campaign-select-btn:hover {
    border-color: var(--primary);
    background: var(--gradient-soft);
    transform: translateY(-2px);
    box-shadow: 0 6px 16px -4px rgba(0, 0, 0, 0.1);
}

.campaign-select-btn.selected {
    border-color: var(--primary);
    background: var(--gradient-soft);
    box-shadow: 0 8px 20px -6px rgba(16, 185, 129, 0.3);
    transform: translateY(-2px);
}

.amount-buttons {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: 16px;
    margin-top: 24px;
}

.amount-btn {
    padding: 18px;
    border: 2px solid var(--border);
    border-radius: var(--radius-md);
    background: var(--bg-primary);
    font-size: 18px;
    font-weight: 700;
    color: var(--text-primary);
    cursor: pointer;
    transition: all var(--transition-smooth);
    box-shadow: var(--shadow-sm);
}

.amount-btn:hover {
    border-color: var(--primary);
    background: var(--gradient-soft);
    transform: translateY(-2px);
    box-shadow: 0 6px 16px -4px rgba(0, 0, 0, 0.1);
}

.amount-btn:active {
    transform: translateY(0) scale(0.98);
    transition: all 100ms cubic-bezier(0.4, 0, 0.2, 1);
}

.payment-mock {
    padding: 48px;
    text-align: center;
    background: var(--bg-secondary);
    border-radius: var(--radius-lg);
    border: 2px dashed var(--border);
}

.payment-mock svg {
    width: 80px;
    height: 80px;
    color: var(--primary);
    margin-bottom: 24px;
}

.payment-summary {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 24px;
    background: var(--bg-primary);
    border-radius: var(--radius-md);
    margin-top: 24px;
    border: 2px solid var(--border-primary);
}

.payment-summary-label {
    font-size: 18px;
    font-weight: 700;
    color: var(--text-secondary);
}

.payment-summary-amount {
    font-size: 32px;
    font-weight: 900;
    color: var(--primary);
}

@media (max-width: 768px) {
    .amount-buttons {
        grid-template-columns: repeat(2, 1fr);
    }
    
    .wizard-step-line {
        width: 60px;
    }
    
    .wizard-step {
        width: 48px;
        height: 48px;
        font-size: 18px;
    }
}

/* ============================================
   PREMIUM UTILITY CLASSES
   ============================================ */

/* Bilgi metinleri - Ana CTA'lardan daha sakin */
.secondary-info,
.info-text {
    font-size: 14px;
    color: var(--text-secondary);
    line-height: 1.6;
    font-weight: 400;
}

.secondary-info svg {
    width: 18px;
    height: 18px;
    color: var(--primary);
    flex-shrink: 0;
}

/* Yetersiz bakiye durumu - Net ama sade */
.btn.insufficient-balance,
.btn[disabled] {
    opacity: 0.6;
    cursor: not-allowed;
    transform: none !important;
    box-shadow: var(--shadow-sm) !important;
    background: var(--bg-tertiary);
    color: var(--text-tertiary);
}

.btn.insufficient-balance:hover,
.btn[disabled]:hover {
    transform: none !important;
    box-shadow: var(--shadow-sm) !important;
}

.btn.insufficient-balance::before,
.btn[disabled]::before {
    display: none;
}

/* Link hover animasyonları */
a:not(.btn):not(.logo) {
    transition: all var(--transition-smooth);
}

/* Smooth page transitions */
@keyframes smoothFadeIn {
    from {
        opacity: 0;
        transform: translateY(6px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

/* Liste öğeleri için stagger animasyon */
.grid > *:nth-child(1) { animation-delay: 0.03s; }
.grid > *:nth-child(2) { animation-delay: 0.06s; }
.grid > *:nth-child(3) { animation-delay: 0.09s; }
.grid > *:nth-child(4) { animation-delay: 0.12s; }
.grid > *:nth-child(5) { animation-delay: 0.15s; }
.grid > *:nth-child(6) { animation-delay: 0.18s; }
.grid > *:nth-child(7) { animation-delay: 0.21s; }
.grid > *:nth-child(8) { animation-delay: 0.24s; }
.grid > *:nth-child(9) { animation-delay: 0.27s; }

/* Hover efektleri için smooth transitions */
.card,
.btn,
.form-input,
.form-select,
.campaign-select-btn,
.amount-btn {
    will-change: transform;
}

/* Focus görünürlüğü */
*:focus-visible {
    outline: 2px solid var(--primary);
    outline-offset: 2px;
    border-radius: 4px;
}

/* ============================================
   DISABLED STATES - Temiz ve Net
   ============================================ */

.btn:disabled,
.btn.disabled {
    opacity: 0.5;
    cursor: not-allowed;
    transform: none !important;
    box-shadow: var(--shadow-sm) !important;
    pointer-events: none;
}

.amount-btn:disabled,
.campaign-select-btn:disabled {
    opacity: 0.4;
    cursor: not-allowed;
    background: var(--bg-tertiary);
    color: var(--text-muted);
    border-color: var(--border);
    transform: none !important;
}

/* Yetersiz bakiye durumu */
.insufficient-balance {
    opacity: 0.5;
    cursor: not-allowed;
    position: relative;
}

.insufficient-balance::after {
    content: 'Yetersiz Bakiye';
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: rgba(0, 0, 0, 0.75);
    color: white;
    padding: 6px 14px;
    border-radius: var(--radius-sm);
    font-size: 13px;
    font-weight: 600;
    white-space: nowrap;
    pointer-events: none;
    opacity: 0;
    transition: opacity var(--transition-fast);
}

.insufficient-balance:hover::after {
    opacity: 1;
}

/* ============================================
   BILGI METİNLERİ - Sakin ve İkincil
   ============================================ */

.info-text {
    font-size: 14px;
    color: var(--text-tertiary);
    line-height: 1.6;
    font-weight: 400;
}

.info-text-sm {
    font-size: 13px;
    color: var(--text-muted);
    opacity: 0.9;
}

/* Puan kazanım bilgileri gibi ikincil bilgiler */
.secondary-info {
    font-size: 14px;
    color: var(--text-tertiary);
    padding: 12px 18px;
    background: var(--bg-secondary);
    border-radius: var(--radius-md);
    border: 1px solid var(--border-light);
    margin-top: 12px;
}

.secondary-info svg {
    width: 16px;
    height: 16px;
    color: var(--text-tertiary);
    opacity: 0.8;
}

/* ============================================
   ENHANCED HOVER EFFECTS - Tüm Etkileşimler
   ============================================ */

/* Link hover geliştirmeleri */
a:not(.btn) {
    transition: all var(--transition-smooth);
}

/* Badge hover */
.badge {
    transition: all var(--transition-smooth);
}

.badge:hover {
    transform: translateY(-1px);
    box-shadow: 0 4px 10px -2px rgba(0, 0, 0, 0.15);
}

/* Kampanya kartı meta bilgileri hover */
.campaign-meta {
    transition: all var(--transition-smooth);
}

/* İkon hover animasyonları */
svg {
    transition: all var(--transition-smooth);
}

/* Tüm interaktif elementlere basılma hissi */
button:active:not(:disabled),
.btn:active:not(:disabled),
.campaign-select-btn:active:not(:disabled),
.amount-btn:active:not(:disabled) {
    transform: translateY(0) scale(0.98) !important;
    transition: all 100ms cubic-bezier(0.4, 0, 0.2, 1) !important;
}

/* ============================================
   SMOOTH SCROLLING
   ============================================ */

html {
    scroll-behavior: smooth;
}

/* ============================================
   LOADING STATES - Akıcı Yükleme
   ============================================ */

@keyframes skeletonLoading {
    0% {
        background-position: 200% 0;
    }
    100% {
        background-position: -200% 0;
    }
}

.skeleton {
    background: linear-gradient(
        90deg,
        var(--bg-secondary) 0%,
        var(--bg-tertiary) 50%,
        var(--bg-secondary) 100%
    );
    background-size: 200% 100%;
    animation: skeletonLoading 1.5s ease-in-out infinite;
    border-radius: var(--radius-md);
}

/* ============================================
   FOCUS STATES - Accessibility
   ============================================ */

*:focus-visible {
    outline: 2px solid var(--primary);
    outline-offset: 3px;
    border-radius: 4px;
}

.btn:focus-visible {
    outline-offset: 4px;
}

/* ============================================
   İYİLİK ALANLARI - KATEGORİ FİLTRE BARI
   ============================================ */

.goodness-areas-filter {
    display: flex;
    gap: 12px;
    margin-bottom: 32px;
    padding: 16px;
    background: var(--bg-secondary);
    border-radius: var(--radius-lg);
    overflow-x: auto;
    scrollbar-width: thin;
    scrollbar-color: var(--primary-light) var(--bg-tertiary);
}

.goodness-areas-filter::-webkit-scrollbar {
    height: 6px;
}

.goodness-areas-filter::-webkit-scrollbar-track {
    background: var(--bg-tertiary);
    border-radius: var(--radius-full);
}

.goodness-areas-filter::-webkit-scrollbar-thumb {
    background: var(--primary-light);
    border-radius: var(--radius-full);
}

.area-filter-btn {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 6px;
    padding: 12px 20px;
    border: 2px solid var(--border);
    background: var(--bg-primary);
    color: var(--text-secondary);
    border-radius: var(--radius-lg);
    cursor: pointer;
    transition: all var(--transition-smooth);
    white-space: nowrap;
    flex-shrink: 0;
    min-width: 100px;
}

.area-icon {
    font-size: 24px;
    filter: grayscale(1);
    transition: all var(--transition-smooth);
}

.area-name {
    font-size: 13px;
    font-weight: 600;
    text-align: center;
}

.area-filter-btn:hover {
    border-color: var(--primary-light);
    background: var(--gradient-soft);
    color: var(--primary);
    transform: translateY(-2px);
}

.area-filter-btn:hover .area-icon {
    filter: grayscale(0);
    transform: scale(1.1);
}

.area-filter-btn.active {
    background: var(--gradient-primary);
    color: white;
    border-color: var(--primary);
    box-shadow: var(--shadow-primary);
}

.area-filter-btn.active .area-icon {
    filter: grayscale(0);
    transform: scale(1.15);
}

/* Eski kategori filtresi - geriye dönük uyumluluk */
.category-filter-bar {
    display: flex;
    gap: 12px;
    margin-bottom: 32px;
    padding: 16px;
    background: var(--bg-secondary);
    border-radius: var(--radius-lg);
    overflow-x: auto;
    scrollbar-width: thin;
    scrollbar-color: var(--primary-light) var(--bg-tertiary);
}

.category-filter-bar::-webkit-scrollbar {
    height: 6px;
}

.category-filter-bar::-webkit-scrollbar-track {
    background: var(--bg-tertiary);
    border-radius: var(--radius-full);
}

.category-filter-bar::-webkit-scrollbar-thumb {
    background: var(--primary-light);
    border-radius: var(--radius-full);
}

.category-filter-btn {
    padding: 10px 20px;
    border: 2px solid var(--border);
    background: var(--bg-primary);
    color: var(--text-secondary);
    font-size: 13px;
    font-weight: 600;
    border-radius: var(--radius-md);
    cursor: pointer;
    transition: all var(--transition-smooth);
    white-space: nowrap;
    flex-shrink: 0;
}

.category-filter-btn:hover {
    border-color: var(--primary-light);
    background: var(--gradient-soft);
    color: var(--primary);
    transform: translateY(-2px);
}

.category-filter-btn.active {
    background: var(--primary);
    color: white;
    border-color: var(--primary);
    box-shadow: 0 2px 8px rgba(16, 185, 129, 0.3);
}

/* ============================================
   GELİŞMİŞ FİLTRELER
   ============================================ */

.advanced-filters-toggle {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    padding: 12px 24px;
    margin-bottom: 20px;
    background: var(--bg-secondary);
    border: 1px solid var(--border);
    border-radius: var(--radius-md);
    cursor: pointer;
    transition: all var(--transition-smooth);
    font-size: 13px;
    font-weight: 600;
    color: var(--text-secondary);
}

.advanced-filters-toggle:hover {
    background: var(--gradient-soft);
    border-color: var(--primary-light);
    color: var(--primary);
}

.toggle-icon {
    width: 18px;
    height: 18px;
    transition: transform var(--transition-smooth);
}

.advanced-filters-container {
    margin-bottom: 40px;
    padding: 24px;
    background: var(--bg-secondary);
    border: 1px solid var(--border);
    border-radius: var(--radius-lg);
    transition: all var(--transition-elegant);
}

/* ============================================
   KATEGORİ ETİKETİ - KAMPANYA KARTLARINDA
   ============================================ */

.campaign-category-label {
    display: inline-block;
    padding: 4px 12px;
    margin-bottom: 12px;
    font-size: 11px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    color: var(--text-tertiary);
    background: var(--bg-tertiary);
    border: 1px solid var(--border);
    border-radius: var(--radius-sm);
    transition: all var(--transition-smooth);
}

.card:hover .campaign-category-label {
    color: var(--primary);
    border-color: var(--primary-light);
    background: var(--gradient-soft);
}

/* ============================================
   ZAMAN BAĞLAMLI KAMPANYALAR
   ============================================ */

.time-sensitive-campaign {
    position: relative;
    border: 2px solid var(--primary-light);
}

.time-sensitive-campaign::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 3px;
    background: var(--gradient-primary);
    transform: scaleX(1);
}

.time-context-badge {
    display: inline-flex;
    align-items: center;
    padding: 6px 14px;
    margin-bottom: 16px;
    font-size: 12px;
    font-weight: 700;
    color: var(--primary-dark);
    background: linear-gradient(135deg, rgba(16, 185, 129, 0.1) 0%, rgba(16, 185, 129, 0.05) 100%);
    border: 1px solid var(--primary-light);
    border-radius: var(--radius-md);
    box-shadow: 0 2px 8px rgba(16, 185, 129, 0.1);
}

.time-sensitive-campaign:hover {
    border-color: var(--primary);
    box-shadow: 0 8px 32px rgba(16, 185, 129, 0.2);
}

/* ============================================
   ÖDÜLLER - ETİK VE YERELLİK VURGUSU
   ============================================ */

.reward-category-label {
    display: inline-block;
    padding: 4px 12px;
    margin-bottom: 12px;
    font-size: 11px;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 0.5px;
    color: var(--text-tertiary);
    background: var(--bg-tertiary);
    border: 1px solid var(--border);
    border-radius: var(--radius-sm);
    transition: all var(--transition-smooth);
}

.reward-card:hover .reward-category-label {
    color: var(--primary);
    border-color: var(--primary-light);
    background: var(--gradient-soft);
}

.social-impact-reward {
    position: relative;
    border: 2px solid rgba(16, 185, 129, 0.2);
}

.social-impact-reward::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 3px;
    background: var(--gradient-primary);
}

.social-impact-reward:hover {
    border-color: var(--primary);
    box-shadow: 0 8px 32px rgba(16, 185, 129, 0.15);
}

.impact-badge {
    display: inline-flex;
    align-items: center;
    padding: 6px 12px;
    margin-bottom: 12px;
    font-size: 12px;
    font-weight: 600;
    color: var(--primary-dark);
    background: linear-gradient(135deg, rgba(16, 185, 129, 0.08) 0%, rgba(16, 185, 129, 0.04) 100%);
    border: 1px solid rgba(16, 185, 129, 0.2);
    border-radius: var(--radius-md);
}

/* ============================================
   KATKI SEÇENEKLERİ - İKİ SEKME YAPISI
   ============================================ */

.contribution-tabs {
    display: flex;
    gap: 16px;
    margin-bottom: 32px;
    padding: 8px;
    background: var(--bg-secondary);
    border-radius: var(--radius-lg);
    box-shadow: var(--shadow-sm);
}

.tab-btn {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 16px 24px;
    font-size: 16px;
    font-weight: 600;
    color: var(--text-secondary);
    background: transparent;
    border: 2px solid transparent;
    border-radius: var(--radius-md);
    cursor: pointer;
    transition: all var(--transition-smooth);
}

.tab-btn:hover {
    color: var(--primary);
    background: rgba(16, 185, 129, 0.05);
}

.tab-btn.active {
    color: var(--primary-dark);
    background: var(--bg-primary);
    border-color: var(--primary);
    box-shadow: var(--shadow-md);
}

.tab-btn svg {
    transition: transform var(--transition-smooth);
}

.tab-btn.active svg {
    transform: scale(1.1);
    filter: drop-shadow(0 2px 4px rgba(16, 185, 129, 0.3));
}

.tab-content-description {
    margin-bottom: 32px;
}

.info-box {
    display: flex;
    align-items: flex-start;
    padding: 16px 20px;
    background: rgba(16, 185, 129, 0.05);
    border-left: 4px solid var(--primary);
    border-radius: var(--radius-md);
    font-size: 14px;
    line-height: 1.6;
    color: var(--text-secondary);
}

.info-box p {
    margin: 0;
}

.info-box svg {
    color: var(--primary);
}

/* Responsive: Mobilde dikey sekme */
@media (max-width: 640px) {
    .contribution-tabs {
        flex-direction: column;
        gap: 8px;
    }
    
    .tab-btn {
        padding: 14px 20px;
        font-size: 15px;
    }
}

/* ============================================
   DESTEKÇİ İŞ ORTAKLARI - MANİFESTO VE GRUPLAR
   ============================================ */

.manifesto-section {
    margin-bottom: 64px;
    padding: 48px;
    background: rgba(16, 185, 129, 0.04);
    border-radius: var(--radius-xl);
    border: 2px solid rgba(16, 185, 129, 0.1);
}

.manifesto-content {
    max-width: 900px;
    margin: 0 auto;
}

.manifesto-text {
    font-size: 16px;
    line-height: 1.8;
    color: var(--text-secondary);
    margin-bottom: 20px;
}

.manifesto-text strong {
    color: var(--primary-dark);
    font-weight: 700;
}

.manifesto-text em {
    color: var(--primary);
    font-style: normal;
    font-weight: 600;
}

.manifesto-highlight {
    padding: 20px 24px;
    background: linear-gradient(135deg, rgba(16, 185, 129, 0.08) 0%, rgba(16, 185, 129, 0.04) 100%);
    border-left: 4px solid var(--primary);
    border-radius: var(--radius-md);
    font-size: 15px;
    line-height: 1.7;
    color: var(--text-primary);
    margin-top: 24px;
}

.manifesto-highlight strong {
    color: var(--primary-dark);
}

/* Destekçi Grup Bölümleri */
.merchant-group-section {
    margin-bottom: 64px;
}

.group-header {
    display: flex;
    align-items: center;
    gap: 20px;
    margin-bottom: 32px;
    padding-bottom: 20px;
    border-bottom: 3px solid var(--border);
}

.group-icon {
    width: 56px;
    height: 56px;
    border-radius: var(--radius-lg);
    background: var(--gradient-primary);
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: var(--shadow-primary);
}

.group-icon svg {
    width: 28px;
    height: 28px;
    color: white;
    stroke-width: 2.5;
}

.group-title {
    font-size: 28px;
    font-weight: 800;
    color: var(--text-primary);
    margin: 0;
}

/* Destekçi Kartları */
.merchant-card {
    background: var(--bg-primary);
    border: 2px solid var(--border);
    border-radius: var(--radius-lg);
    padding: 24px;
    transition: all var(--transition-smooth);
}

.merchant-card:hover {
    border-color: var(--primary-light);
    box-shadow: var(--shadow-md);
    transform: translateY(-2px);
}

.merchant-card-body {
    display: flex;
    flex-direction: column;
    gap: 16px;
}

.merchant-name {
    font-size: 20px;
    font-weight: 700;
    color: var(--text-primary);
    margin: 0;
}

.merchant-info {
    display: flex;
    flex-direction: column;
    gap: 8px;
}

.merchant-detail {
    display: flex;
    align-items: center;
    gap: 8px;
    font-size: 14px;
    color: var(--text-secondary);
}

.icon-sm {
    width: 16px;
    height: 16px;
    color: var(--primary);
    flex-shrink: 0;
}

.contribution-badge {
    display: inline-flex;
    align-items: center;
    gap: 8px;
}


.profile-avatar {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    background: linear-gradient(135deg, var(--primary), var(--primary-light));
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 8px;
    box-shadow: var(--shadow-sm);
}

.profile-avatar svg {
    width: 40px;
    height: 40px;
    color: white;
}

.profile-name {
    font-size: 24px;
    font-weight: 700;
    color: var(--text-primary);
    margin: 0;
}

.profile-email {
    font-size: 14px;
    color: var(--text-tertiary);
    margin: 0;
}

/* Impact Summary Card */
.impact-summary-card {
    padding: 24px;
}

.impact-summary-text {
    font-size: 18px;
    color: var(--text-secondary);
    line-height: 1.6;
    text-align: center;
    margin: 0 0 24px 0;
}

.impact-summary-text strong {
    color: var(--primary);
    font-weight: 700;
}

/* Impact Stats Grid */
.impact-stats-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
    gap: 24px;
    margin-bottom: 32px;
}

.impact-stat-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 8px;
    text-align: center;
}

.impact-stat-icon {
    font-size: 48px;
    line-height: 1;
    margin-bottom: 4px;
}

.impact-stat-value {
    font-size: 32px;
    font-weight: 700;
    color: var(--primary);
    line-height: 1;
}

.impact-stat-label {
    font-size: 14px;
    color: var(--text-secondary);
    font-weight: 500;
}

/* Supported Areas List */
.supported-areas-list {
    display: flex;
    flex-wrap: wrap;
    gap: 12px;
    margin-top: 16px;
}

.supported-area-item {
    display: inline-flex;
    align-items: center;
    gap: 8px;
    background: var(--bg-secondary);
    padding: 8px 16px;
    border-radius: var(--radius-full);
    font-size: 14px;
    font-weight: 500;
    transition: all var(--transition-smooth);
}

.supported-area-item:hover {
    background: var(--primary-lightest);
    transform: translateY(-1px);
}

.area-badge {
    font-size: 18px;
    line-height: 1;
}

.area-count {
    color: var(--primary);
    font-weight: 600;
}

/* Impact Timeline */
.impact-timeline {
    position: relative;
    padding-left: 32px;
}

.impact-timeline::before {
    content: '';
    position: absolute;
    left: 8px;
    top: 8px;
    bottom: 8px;
    width: 2px;
    background: var(--border);
}

.timeline-item {
    position: relative;
    padding-bottom: 24px;
}

.timeline-item:last-child {
    padding-bottom: 0;
}

.timeline-dot {
    position: absolute;
    left: -28px;
    top: 4px;
    width: 12px;
    height: 12px;
    border-radius: 50%;
    background: var(--primary);
    border: 3px solid var(--bg-primary);
    z-index: 1;
}

.timeline-content {
    display: flex;
    flex-direction: column;
    gap: 4px;
}

.timeline-date {
    font-size: 12px;
    color: var(--text-tertiary);
    font-weight: 500;
}

.timeline-campaign {
    font-size: 15px;
    color: var(--text-primary);
    font-weight: 600;
    line-height: 1.4;
}

.timeline-category {
    display: inline-flex;
    align-items: center;
    gap: 6px;
    font-size: 13px;
    color: var(--text-secondary);
    margin-top: 4px;
}

.timeline-points {
    font-size: 14px;
    color: var(--primary);
    font-weight: 600;
    margin-top: 4px;
}

/* Privacy Options */
.privacy-option {
    display: flex;
    align-items: flex-start;
    gap: 12px;
    padding: 16px;
    background: var(--bg-secondary);
    border-radius: var(--radius-md);
    margin-bottom: 12px;
    transition: all var(--transition-smooth);
}

.privacy-option:hover {
    background: var(--primary-lightest);
}

.privacy-option input[type="checkbox"] {
    margin-top: 2px;
    width: 18px;
    height: 18px;
    cursor: pointer;
    accent-color: var(--primary);
}

.privacy-label {
    flex: 1;
    cursor: pointer;
    user-select: none;
}

.privacy-label strong {
    display: block;
    color: var(--text-primary);
    font-weight: 600;
    margin-bottom: 4px;
}

.privacy-label small {
    color: var(--text-secondary);
    font-size: 13px;
    line-height: 1.5;
}

/* Empty State */
.empty-state {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    padding: 64px 24px;
    color: var(--text-tertiary);
}

.empty-state-icon {
    font-size: 64px;
    margin-bottom: 16px;
    opacity: 0.5;
}

.empty-state p {
    font-size: 16px;
    color: var(--text-secondary);
    max-width: 320px;
    margin: 0;
}

/* Responsive Adjustments */
@media (max-width: 768px) {
    .impact-stats-grid {
        grid-template-columns: 1fr;
        gap: 20px;
    }
    
    .impact-stat-icon {
        font-size: 40px;
    }
    
    .impact-stat-value {
        font-size: 28px;
    }
    
    .impact-timeline {
        padding-left: 28px;
    }
    
    .timeline-dot {
        left: -24px;
        width: 10px;
        height: 10px;
    }
    
    .supported-areas-list {
        gap: 8px;
    }
    
    .supported-area-item {
        padding: 6px 12px;
        font-size: 13px;
    }
}

/* ========================================
   PROFILE & IMPACT STYLES
   ======================================== */

/* Profile Identity Card */
.profile-identity-card {
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    gap: 16px;
    padding: 32px 24px;
}

.profile-avatar {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    background: linear-gradient(135deg, var(--primary), var(--primary-light));
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 8px;
    box-shadow: var(--shadow-sm);
}

.profile-avatar svg {
    width: 40px;
    height: 40px;
    color: white;
}

.profile-name {
    font-size: 24px;
    font-weight: 700;
    color: var(--text-primary);
    margin: 0;
}

.profile-email {
    font-size: 14px;
    color: var(--text-tertiary);
    margin: 0;
}

/* Impact Summary Card */
.impact-summary-card {
    padding: 24px;
}

.impact-summary-text {
    font-size: 18px;
    color: var(--text-secondary);
    line-height: 1.6;
    text-align: center;
    margin: 0 0 24px 0;
}

.impact-summary-text strong {
    color: var(--primary);
    font-weight: 700;
}

/* Impact Stats Grid */
.impact-stats-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
