    margin-bottom: 32px;
}

.impact-stat-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 8px;
    text-align: center;
}

.impact-stat-icon {
    font-size: 48px;
    line-height: 1;
    margin-bottom: 4px;
}

.impact-stat-value {
    font-size: 32px;
    font-weight: 700;
    color: var(--primary);
    line-height: 1;
}

.impact-stat-label {
    font-size: 14px;
    color: var(--text-secondary);
    font-weight: 500;
}

/* Supported Areas List */
.supported-areas-list {
    display: flex;
    flex-wrap: wrap;
    gap: 12px;
    margin-top: 16px;
}

.supported-area-item {
    display: inline-flex;
    align-items: center;
    gap: 8px;
    background: var(--bg-secondary);
    padding: 8px 16px;
    border-radius: var(--radius-full);
    font-size: 14px;
    font-weight: 500;
    transition: all var(--transition-smooth);
}

.supported-area-item:hover {
    background: var(--primary-lightest);
    transform: translateY(-1px);
}

.area-badge {
    font-size: 18px;
    line-height: 1;
}

.area-count {
    color: var(--primary);
    font-weight: 600;
}

/* Impact Timeline */
.impact-timeline {
    position: relative;
    padding-left: 32px;
}

.impact-timeline::before {
    content: '';
    position: absolute;
    left: 8px;
    top: 8px;
    bottom: 8px;
    width: 2px;
    background: var(--border);
}

.timeline-item {
    position: relative;
    padding-bottom: 24px;
}

.timeline-item:last-child {
    padding-bottom: 0;
}

.timeline-dot {
    position: absolute;
    left: -28px;
    top: 4px;
    width: 12px;
    height: 12px;
    border-radius: 50%;
    background: var(--primary);
    border: 3px solid var(--bg-primary);
    z-index: 1;
}

.timeline-content {
    display: flex;
    flex-direction: column;
    gap: 4px;
}

.timeline-date {
    font-size: 12px;
    color: var(--text-tertiary);
    font-weight: 500;
}

.timeline-campaign {
    font-size: 15px;
    color: var(--text-primary);
    font-weight: 600;
    line-height: 1.4;
}

.timeline-category {
    display: inline-flex;
    align-items: center;
    gap: 6px;
    font-size: 13px;
    color: var(--text-secondary);
    margin-top: 4px;
}

.timeline-points {
    font-size: 14px;
    color: var(--primary);
    font-weight: 600;
    margin-top: 4px;
}

/* Privacy Options */
.privacy-option {
    display: flex;
    align-items: flex-start;
    gap: 12px;
    padding: 16px;
    background: var(--bg-secondary);
    border-radius: var(--radius-md);
    margin-bottom: 12px;
    transition: all var(--transition-smooth);
}

.privacy-option:hover {
    background: var(--primary-lightest);
}

.privacy-option input[type="checkbox"] {
    margin-top: 2px;
    width: 18px;
    height: 18px;
    cursor: pointer;
    accent-color: var(--primary);
}

.privacy-label {
    flex: 1;
    cursor: pointer;
    user-select: none;
}

.privacy-label strong {
    display: block;
    color: var(--text-primary);
    font-weight: 600;
    margin-bottom: 4px;
}

.privacy-label small {
    color: var(--text-secondary);
    font-size: 13px;
    line-height: 1.5;
}

/* Empty State */
.empty-state {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    padding: 64px 24px;
    color: var(--text-tertiary);
}

.empty-state-icon {
    font-size: 64px;
    margin-bottom: 16px;
    opacity: 0.5;
}

.empty-state p {
    font-size: 16px;
    color: var(--text-secondary);
    max-width: 320px;
    margin: 0;
}

/* Contribution Badge (continuation from above) */
.contribution-badge {
    padding: 10px 16px;
    background: linear-gradient(135deg, rgba(16, 185, 129, 0.08) 0%, rgba(16, 185, 129, 0.04) 100%);
    border: 1px solid rgba(16, 185, 129, 0.2);
    border-radius: var(--radius-md);
    font-size: 13px;
    font-weight: 600;
    color: var(--primary-dark);
}

.contribution-description {
    font-size: 14px;
    line-height: 1.6;
    color: var(--text-secondary);
    margin: 0;
    padding-top: 8px;
    border-top: 1px solid var(--border-light);
}

/* Responsive */
@media (max-width: 768px) {
    .manifesto-section {
        padding: 32px 24px;
    }
    
    .group-header {
        flex-direction: column;
        align-items: flex-start;
        gap: 16px;
    }
    
    .group-title {
        font-size: 22px;
    }
}

/* ============================================
   PRINT STYLES
   ============================================ */

@media print {
    header, footer, .btn, nav {
        display: none;
    }
    
    body {
        background: white;
    }
    
    .card {
        break-inside: avoid;
        box-shadow: none;
        border: 1px solid #ddd;
    }
}

/* ============================================
   RESPONSIVE DESIGN - MOBILE FIRST
   ============================================ */

/* Mobile Phones (< 576px) - Base styles */
@media (max-width: 575px) {
    /* Container padding for mobile */
    .container {
        padding-left: 16px;
        padding-right: 16px;
    }
    
    /* Header Mobile */
    .header-content {
        height: 70px;
        gap: 16px;
    }
    
    .logo {
        gap: 10px;
    }
    
    .logo-icon {
        width: 48px;
        height: 48px;
    }
    
    .logo-icon svg {
        width: 28px;
        height: 28px;
    }
    
    .logo-text {
        font-size: 20px;
    }
    
    .mobile-menu {
        display: block;
        top: 70px;
    }
    
    /* Page title adjustments */
    .page-title {
        font-size: 28px;
        line-height: 1.2;
        margin-bottom: 12px;
    }
    
    .page-description {
        font-size: 14px;
        line-height: 1.5;
    }
    
    /* Stats grid - single column */
    .stats-grid {
        grid-template-columns: 1fr !important;
        gap: 12px !important;
    }
    
    /* Stat cards mobile adjustments */
    .stat-card {
        padding: 20px 16px;
        min-height: 140px;
    }
    
    .stat-icon {
        width: 40px;
        height: 40px;
        margin-bottom: 12px;
    }
    
    .stat-value {
        font-size: 20px;
        margin-bottom: 6px;
    }
    
    .stat-label {
        font-size: 11px;
    }
    
    /* Campaign grid - single column */
    .campaign-grid {
        grid-template-columns: 1fr !important;
        gap: 16px !important;
    }
    
    /* Campaign cards */
    .campaign-card {
        margin-bottom: 16px;
    }
    
    /* Header adjustments */
    header nav {
        flex-direction: column;
        gap: 12px;
    }
    
    header .logo {
        font-size: 20px;
    }
    
    header .nav-links {
        flex-direction: column;
        width: 100%;
        gap: 8px;
    }
    
    header .nav-links a {
        width: 100%;
        text-align: center;
        padding: 12px;
    }
    
    /* Buttons full width on mobile */
    .btn {
        width: 100%;
        padding: 14px 20px;
        font-size: 14px;
    }
    
    /* Cards */
    .card {
        margin-bottom: 16px;
        padding: 16px;
        border-radius: 12px;
    }
    
    .card-body {
        padding: 16px;
    }
    
    /* Tables - make them scrollable */
    table {
        display: block;
        overflow-x: auto;
        white-space: nowrap;
        font-size: 13px;
    }
    
    table th,
    table td {
        padding: 10px !important;
        font-size: 13px !important;
    }
    
    /* Modal adjustments */
    .modal-content {
        width: 95%;
        margin: 20px auto;
        padding: 20px;
    }
    
    /* Form inputs */
    input, select, textarea {
        font-size: 16px; /* Prevents zoom on iOS */
    }
    
    /* Footer */
    footer {
        padding: 24px 16px;
        text-align: center;
    }
    
    footer .footer-links {
        flex-direction: column;
        gap: 12px;
    }
    
    /* Donation cards */
    .donation-card {
        padding: 16px;
    }
    
    /* Progress bars */
    .progress-info {
        flex-direction: column;
        align-items: flex-start;
        gap: 8px;
    }
    
    /* Impact stats grid */
    .impact-stats-grid {
        grid-template-columns: 1fr !important;
        gap: 12px !important;
    }
    
    /* Wallet widget */
    .wallet-widget {
        padding: 16px;
    }
    
    /* Hero section */
    .hero-section {
        padding: 40px 20px;
        text-align: center;
    }
    
    .hero-title {
        font-size: 32px;
    }
    
    .hero-subtitle {
        font-size: 16px;
    }
}

/* Tablets (576px - 768px) */
@media (min-width: 576px) and (max-width: 767px) {
    /* Header */
    .mobile-menu-btn {
        display: flex;
    }
    
    .mobile-menu {
        display: block;
    }
    
    .stats-grid {
        grid-template-columns: repeat(2, 1fr) !important;
        gap: 16px !important;
    }
    
    .campaign-grid {
        grid-template-columns: repeat(2, 1fr) !important;
        gap: 16px !important;
    }
    
    .impact-stats-grid {
        grid-template-columns: repeat(2, 1fr) !important;
    }
    
    .page-title {
        font-size: 32px;
    }
    
    .stat-card {
        min-height: 150px;
    }
}

/* Tablets Landscape (768px - 991px) */
@media (min-width: 768px) and (max-width: 991px) {
    .container {
        max-width: 720px;
        padding: 0 24px;
    }
    
    .stats-grid {
        grid-template-columns: repeat(2, 1fr) !important;
        gap: 20px !important;
    }
    
    .campaign-grid {
        grid-template-columns: repeat(2, 1fr) !important;
        gap: 20px !important;
    }
    
    .impact-stats-grid {
        grid-template-columns: repeat(2, 1fr) !important;
    }
    
    .page-title {
        font-size: 36px;
    }
    
    header nav {
        flex-direction: row;
        justify-content: space-between;
    }
    
    header .nav-links {
        flex-direction: row;
        width: auto;
        gap: 16px;
    }
    
    .btn {
        width: auto;
    }
}

/* Small Desktops (992px - 1199px) */
@media (min-width: 992px) and (max-width: 1199px) {
    .container {
        max-width: 960px;
    }
    
    .stats-grid {
        grid-template-columns: repeat(4, 1fr) !important;
        gap: 20px !important;
    }
    
    .campaign-grid {
        grid-template-columns: repeat(3, 1fr) !important;
        gap: 24px !important;
    }
    
    .impact-stats-grid {
        grid-template-columns: repeat(3, 1fr) !important;
    }
}

/* Large Desktops (>= 1200px) */
@media (min-width: 1200px) {
    .container {
        max-width: 1140px;
    }
    
    .stats-grid {
        grid-template-columns: repeat(4, 1fr) !important;
        gap: 24px !important;
    }
    
    .campaign-grid {
        grid-template-columns: repeat(3, 1fr) !important;
        gap: 28px !important;
    }
    
    .impact-stats-grid {
        grid-template-columns: repeat(3, 1fr) !important;
    }
}

/* Extra Large Desktops (>= 1400px) */
@media (min-width: 1400px) {
    .container {
        max-width: 1320px;
    }
    
    .campaign-grid {
        grid-template-columns: repeat(4, 1fr) !important;
    }
}

/* Landscape Phone Orientation */
@media (max-height: 600px) and (orientation: landscape) {
    header {
        position: sticky;
        top: 0;
        z-index: 100;
    }
    
    .hero-section {
        padding: 30px 20px;
    }
    
    .hero-title {
        font-size: 28px;
    }
}

/* Touch Device Optimizations */
@media (hover: none) and (pointer: coarse) {
    /* Increase tap targets for touch devices */
    .btn,
    button,
    a {
        min-height: 44px;
        min-width: 44px;
    }
    
    /* Remove hover effects on touch devices */
    .stat-card:hover,
    .campaign-card:hover,
    .btn:hover {
        transform: none;
    }
    
    /* Make interactive elements more obvious */
    .btn {
        box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
    }
}

/* High DPI Screens (Retina) */
@media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) {
    /* Sharper borders and shadows */
    .card {
        border-width: 0.5px;
    }
    
    img {
        image-rendering: -webkit-optimize-contrast;
    }
}

/* Reduced Motion Preference */
@media (prefers-reduced-motion: reduce) {
    *,
    *::before,
    *::after {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
        scroll-behavior: auto !important;
    }
}

/* Dark Mode Support (if user prefers) */
@media (prefers-color-scheme: dark) {
    /* Dark mode can be implemented here if needed */
    /* For now, keeping light theme */
}

/* Print Optimizations - Additional */
@media print {
    /* Hide navigation and interactive elements */
    .btn,
    button,
    .wallet-widget,
    .stat-card:hover::before,
    .stat-card:hover::after {
        display: none;
    }
    
    /* Optimize spacing for print */
    .stats-grid {
        grid-template-columns: repeat(2, 1fr) !important;
        gap: 10px !important;
    }
    
    .page-title {
        font-size: 24px;
        color: black;
    }
    
    /* Remove backgrounds */
    .stat-card,
    .card {
        background: white !important;
        box-shadow: none !important;
    }
}

/* ============================================
   IMPACT CARD - ETKİ KARTI
   ============================================ */

.impact-card-page {
    max-width: 800px;
    margin: 0 auto;
}

.page-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 32px;
}

.period-selector {
    display: flex;
    gap: 12px;
    margin-bottom: 32px;
    justify-content: center;
}

.period-btn {
    padding: 12px 24px;
    border-radius: var(--radius-md);
    background: var(--bg-secondary);
    color: var(--text-secondary);
    text-decoration: none;
    font-weight: 600;
    transition: all var(--transition-smooth);
    border: 2px solid transparent;
}

.period-btn:hover {
    background: var(--bg-tertiary);
    color: var(--text-primary);
}

.period-btn.active {
    background: var(--primary);
    color: white;
    border-color: var(--primary);
}

.impact-card {
    background: linear-gradient(135deg, var(--primary) 0%, #059669 100%) !important;
    border-radius: var(--radius-2xl) !important;
    padding: 40px !important;
    color: white !important;
    box-shadow: 0 20px 60px rgba(16, 185, 129, 0.3) !important;
    margin-bottom: 40px !important;
    border: none !important;
}

.impact-card .card-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 24px;
    padding-bottom: 24px;
    padding-top: 0 !important;
    padding-left: 0 !important;
    padding-right: 0 !important;
    border-bottom: 1px solid rgba(255, 255, 255, 0.2);
    background: transparent !important;
}

.impact-card .card-logo {
    font-size: 24px;
    font-weight: 900;
    letter-spacing: 2px;
    color: white;
}

.impact-card .card-period {
    font-size: 14px;
    font-weight: 600;
    color: rgba(255, 255, 255, 0.9);
    text-transform: uppercase;
    letter-spacing: 1px;
}

.impact-card .card-date-range {
    font-size: 14px;
    color: rgba(255, 255, 255, 0.8);
    margin-bottom: 32px;
}

.impact-card .card-stats {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 24px;
    margin-bottom: 32px;
}

@media (min-width: 768px) {
    .impact-card .card-stats {
        grid-template-columns: repeat(4, 1fr);
    }
}

.impact-card .stat-item {
    text-align: center;
}

.impact-card .stat-value {
    font-size: 28px;
    font-weight: 900;
    color: white;
    margin-bottom: 8px;
    line-height: 1;
}

.impact-card .stat-label {
    font-size: 12px;
    color: rgba(255, 255, 255, 0.8);
    text-transform: uppercase;
    letter-spacing: 1px;
    font-weight: 600;
}

.impact-card .card-impact-statement {
    background: rgba(255, 255, 255, 0.15);
    backdrop-filter: blur(10px);
    padding: 24px;
    border-radius: var(--radius-lg);
    margin-bottom: 32px;
    text-align: center;
}

.impact-card .quote-icon {
    font-size: 32px;
    margin-bottom: 12px;
}

.impact-card .card-impact-statement p {
    font-size: 16px;
    line-height: 1.6;
    color: white;
    margin: 0;
}

.impact-card .card-categories h4 {
    font-size: 16px;
    font-weight: 700;
    color: white;
    margin-bottom: 16px;
}

.impact-card .category-tags {
    display: flex;
    flex-wrap: wrap;
    gap: 8px;
}

.impact-card .category-tag {
    background: rgba(255, 255, 255, 0.2);
    color: white;
    padding: 8px 16px;
    border-radius: var(--radius-full);
    font-size: 13px;
    font-weight: 600;
    backdrop-filter: blur(10px);
}

.impact-card .card-footer {
    margin-top: 32px;
    padding-top: 24px;
    padding-bottom: 0 !important;
    padding-left: 0 !important;
    padding-right: 0 !important;
    border-top: 1px solid rgba(255, 255, 255, 0.2);
    text-align: center;
    background: transparent !important;
}

.impact-card .card-watermark {
    font-size: 12px;
    color: rgba(255, 255, 255, 0.6);
    font-weight: 600;
    letter-spacing: 1px;
}

.share-options {
    text-align: center;
    margin-top: 40px;
}

.share-options h3 {
    font-size: 24px;
    font-weight: 700;
    color: var(--text-primary);
    margin-bottom: 12px;
}

.share-options p {
    color: var(--text-secondary);
    margin-bottom: 24px;
}

.share-buttons {
    display: flex;
    gap: 12px;
    justify-content: center;
    flex-wrap: wrap;
}

.btn-share {
    padding: 12px 24px;
    border-radius: var(--radius-md);
    background: var(--bg-secondary);
    color: var(--text-primary);
    border: 2px solid var(--border);
    font-weight: 600;
    cursor: pointer;
    transition: all var(--transition-smooth);
    display: inline-flex;
    align-items: center;
}

.btn-share:hover {
    background: var(--bg-tertiary);
    border-color: var(--primary-light);
    transform: translateY(-2px);
}

.btn-share-primary {
    background: var(--primary);
    color: white;
    border-color: var(--primary);
}

.btn-share-primary:hover {
    background: var(--primary-dark);
    border-color: var(--primary-dark);
}

/* Share Modal */
.share-modal {
    position: fixed;
    inset: 0;
    z-index: 9999;
    display: flex;
    align-items: center;
    justify-content: center;
}

.share-modal-overlay {
    position: absolute;
    inset: 0;
    background: rgba(0, 0, 0, 0.5);
    backdrop-filter: blur(4px);
}

.share-modal-content {
    position: relative;
    background: var(--bg-primary);
    border-radius: var(--radius-xl);
    max-width: 500px;
    width: 90%;
    max-height: 80vh;
    overflow-y: auto;
    box-shadow: 0 25px 50px -12px rgba(0, 0, 0, 0.25);
}

.share-modal-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 24px;
    border-bottom: 1px solid var(--border);
}

.share-modal-header h3 {
    font-size: 20px;
    font-weight: 700;
    color: var(--text-primary);
    margin: 0;
}

.share-modal-close {
    background: none;
    border: none;
    font-size: 32px;
    color: var(--text-tertiary);
    cursor: pointer;
    padding: 0;
    width: 32px;
    height: 32px;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: var(--radius-md);
    transition: all var(--transition-smooth);
}

.share-modal-close:hover {
    background: var(--bg-secondary);
    color: var(--text-primary);
}

.share-modal-body {
    padding: 24px;
}

.social-share-grid {
    display: grid;
    gap: 12px;
}

.share-option-btn {
    display: flex;
    align-items: center;
    gap: 16px;
    padding: 16px;
    background: var(--bg-secondary);
    border: 2px solid var(--border);
    border-radius: var(--radius-lg);
    cursor: pointer;
    transition: all var(--transition-smooth);
    text-align: left;
    width: 100%;
}

.share-option-btn:hover {
    background: var(--bg-tertiary);
    border-color: var(--primary-light);
    transform: translateX(4px);
}

.share-icon {
    width: 48px;
    height: 48px;
    display: flex;
    align-items: center;
    justify-content: center;
    background: var(--bg-primary);
    border-radius: var(--radius-md);
    font-size: 24px;
    flex-shrink: 0;
}

.share-option-title {
    font-size: 16px;
    font-weight: 700;
    color: var(--text-primary);
    margin-bottom: 4px;
}

.share-option-desc {
    font-size: 13px;
    color: var(--text-secondary);
}
